{
  "name": "Job Evaluator",
  "version": "1.0",
  "description": "Evaluates job decriptions with user inputted skills to provide a score for how suitable the job is.",
  "permissions":["storage"],
  "background":{
    "scripts": ["text_search.js"],
    "persistent":false
  },
  "browser_action":{
    "default_popup":"popup.html"
  },
  
  "content_scripts": [
    {
        "matches": [
            "*://*/*"
        ],
        "js": [
            "content.js",
            "float_element.js"
        ],
        "run_at": "document_end"
    }
  ],
  "manifest_version": 2,
  "web_accessible_resources": [
    "float_element.html"
  ]
}
